<body style="background-color: black;">


   <script>


      function findPairs(array, num) {

         let myMap = new Map();

         for (let i = 0; i <= array.length; i++) {
            for (let j = i; j <= array.length; j++) {
               if (array[i] + array[j] === num) {
                  myMap.set(array[i], array[j])
               }
            }
         }
         console.log([...myMap])
      }

      const findSumPairs = (arr, value) => {
         let sumsLookup = {};
         let output = [];

         for (let i = 0; i < arr.length; i++) {
            let targetVal = value - arr[i];
            /*
            6 = 7-1 
            2 = 7-5 
            1 = 7-6 
            6 = 7-1 
            7 = 7-0 
            6 = 7-1 
            */

            if (sumsLookup[targetVal]) {
               output.push([arr[i], targetVal]);
            }

            /*
            1 true *       push 6 1 
            5 true
            6 true * *     push 1 6 / 1 6 
            1 true
            0 true
            1 true
            */

            sumsLookup[arr[i]] = true;
         }

         return output;
      }

      let array = [1, 5, 6, 1, 0, 1];
      findPairs(array, 7)

      console.log(findSumPairs(array, 7));


   </script>


</body>

</html>